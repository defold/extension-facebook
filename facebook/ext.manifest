name: FacebookExtExternal

platforms:
    android:
        context:
            aaptExtraPackages: ['com.facebook', 'com.facebook.common', 'com.facebook.login', 'com.facebook.share']

    armv7-ios:
        context:
            flags: ['-std=c++11']

    arm64-ios:
        context:
            flags: ['-std=c++11']

    x86_64-ios:
        context:
            flags: ['-std=c++11']
